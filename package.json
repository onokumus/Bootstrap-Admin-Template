{
  "name": "bootstrap-admin-template",
  "description": "Free Admin Template Based On Twitter Bootstrap 3.x",
  "version": "3.0.0-alpha",
  "homepage": "https://github.com/puikinsh/Bootstrap-Admin-Template",
  "author": "puikinsh (http://github.com/puikinsh)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/puikinsh/Bootstrap-Admin-Template.git"
  },
  "bugs": {
    "url": "https://github.com/puikinsh/Bootstrap-Admin-Template/issues"
  },
  "license": "MIT",
  "keywords": [
    "twbs",
    "assemble",
    "gulp",
    "less",
    "admin",
    "template"
  ],
  "main": "dist/core.js",
  "style": "dist/main.css",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "assets": "./node_modules/.bin/gulp",
    "pages": "./node_modules/.bin/assemble --file assemblefile.js",
    "build": "npm run assets && npm run pages",
    "mainrtldist": "rtlcss dist/main.css dist/main.rtl.css && csso dist/main.rtl.css dist/main.rtl.min.css",
    "mainrtlpublic": "rtlcss dist/main.css public/assets/css/main.rtl.css && csso dist/main.rtl.css public/assets/css/main.rtl.min.css",
    "rtl": "npm run mainrtldist && npm run mainrtlpublic",
    "buildrtl": "npm run build && npm run rtl"
  },
  "devDependencies": {
    "assemble": "^0.24.3",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.23.6",
    "css-flip": "^0.5.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-extname": "^0.2.2",
    "gulp-format-md": "^1.0.0",
    "gulp-header": "^2.0.5",
    "gulp-if": "^2.0.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-open": "^3.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass":"^4.0.1",
    "gulp-size": "^3.0.0",
    "gulp-uglify": "^3.0.0",
    "handlebars-helpers": "^0.10.0",
    "helper-markdown": "^1.0.0",
    "helper-md": "^0.2.2",
    "rtlcss": "^2.2.1"
  },
  "dependencies": {
    "noty": "^3.2.0-beta",
    "clipboard": "^2.0.0",
    "fullcalendar": "^3.9.0",
    "gmaps": "^0.4.24",
    "jquery-validation": "^1.17.0"
  },
  "verb": {
    "toc": true,
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ]
  }
}
